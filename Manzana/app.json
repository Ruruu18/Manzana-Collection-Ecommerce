{
  "expo": {
    "name": "Manzana Collection",
    "slug": "manzan-collecion",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#D4587A"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.manzana.collection",
      "infoPlist": {
        "UIBackgroundModes": [
          "remote-notification"
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#D4587A"
      },
      "package": "com.manzana.collection",
      "permissions": [
        "android.permission.RECEIVE_BOOT_COMPLETED",
        "android.permission.VIBRATE",
        "android.permission.POST_NOTIFICATIONS",
        "android.permission.RECORD_AUDIO",
        "android.permission.WAKE_LOCK"
      ],
      "googleServicesFile": "./google-services.json"
    },
    "notification": {
      "icon": "./assets/icon.png",
      "color": "#D4587A",
      "androidMode": "default",
      "androidCollapsedTitle": "{{unread_count}} new notifications"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-secure-store",
      [
        "expo-notifications",
        {
          "icon": "./assets/icon.png",
          "color": "#D4587A",
          "mode": "production"
        }
      ],
      "expo-image-picker",
      "expo-font",
      "expo-task-manager"
    ],
    "scheme": "manzana",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "extra": {
      "supabaseUrl": "https://fuqsweradcynpbgarwoc.supabase.co",
      "supabaseAnonKey": "eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9.eyJpc3MiOiJzdXBhYmFzZSIsInJlZiI6ImZ1cXN3ZXJhZGN5bnBiZ2Fyd29jIiwicm9sZSI6ImFub24iLCJpYXQiOjE3NTc1OTQ2MzcsImV4cCI6MjA3MzE3MDYzN30.bEUieVgxnM1xaBgMaj8AM07vSbdKP_Ti_pWKUGULXzI",
      "eas": {
        "projectId": "e833d979-5ffc-44eb-9209-a261eaab5bc0"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "owner": "mharlie18"
  }
}
