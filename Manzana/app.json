{
  "expo": {
    "name": "Manzana Collection",
    "slug": "manzan-collecion",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#D4587A"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.manzana.collection",
      "infoPlist": {
        "UIBackgroundModes": ["remote-notification"]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#D4587A"
      },
      "package": "com.manzana.collection",
      "permissions": [
        "RECEIVE_BOOT_COMPLETED",
        "VIBRATE",
        "POST_NOTIFICATIONS"
      ],
      "googleServicesFile": "./google-services.json",
      "useNextNotificationsApi": true
    },
    "notification": {
      "icon": "./assets/icon.png",
      "color": "#D4587A",
      "androidMode": "default",
      "androidCollapsedTitle": "{{unread_count}} new notifications"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-secure-store",
      [
        "expo-notifications",
        {
          "icon": "./assets/icon.png",
          "color": "#D4587A",
          "mode": "production"
        }
      ],
      "expo-image-picker",
      "expo-font"
    ],
    "scheme": "manzana",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "extra": {
      "eas": {
        "projectId": "ad21d7ba-5062-448a-ac2c-19e369325727"
      },
      "supabaseUrl": "https://fuqsweradcynpbgarwoc.supabase.co",
