# Supabase Configuration
EXPO_PUBLIC_SUPABASE_URL=your_supabase_project_url_here
EXPO_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# App Configuration
EXPO_PUBLIC_APP_ENV=development
EXPO_PUBLIC_API_BASE_URL=https://your-api-domain.com/api

# Analytics (Optional)
EXPO_PUBLIC_ANALYTICS_ID=your_analytics_id_here

# Push Notifications (Optional)
EXPO_PUBLIC_PUSH_NOTIFICATION_KEY=your_push_notification_key_here

# Deep Linking
EXPO_PUBLIC_DEEP_LINK_SCHEME=manzana

# Development Settings
EXPO_PUBLIC_DEBUG_MODE=true
EXPO_PUBLIC_LOG_LEVEL=debug

# Third Party Services (Optional)
EXPO_PUBLIC_STRIPE_PUBLISHABLE_KEY=pk_test_your_stripe_key_here
EXPO_PUBLIC_GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here

# Image Upload Settings
EXPO_PUBLIC_MAX_IMAGE_SIZE=1024
EXPO_PUBLIC_IMAGE_QUALITY=0.8

# Cache Settings
EXPO_PUBLIC_CACHE_TTL=3600
